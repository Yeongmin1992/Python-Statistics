    "def fib_chick(n):\n",
    "    a, b = 0, 1\n",
    "    ls = []\n",
    "    while a < n:\n",
    "        ls.append(a)\n",
    "        a, b = b, a+b\n",
    "    if n == a:\n",
    "        print(ls[-1])\n",
    "    else:\n",
    "        print(ls[-2]+ls[n-ls[-1]])\n",
    "        \n",
    "fib_chick(13)"
   ]
